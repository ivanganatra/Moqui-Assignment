<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/service-definition-2.1.xsd">

    <!-- Example & Related Services -->
    <service verb="create" noun="TutorialEntity"  displayName="Create an Example" type="entity-auto" allow-remote="true">
        <in-parameters>
            <auto-parameters include="nonpk"/>
            <parameter name="tutorialName" required="true"/>
            <parameter name="tutorialDate" required="true"/>
            <parameter name="tutorialPrice" required="true"/>
            <parameter name="tutorialQuantity" required="true"/>
        </in-parameters>
        <out-parameters>
            <auto-parameters include="pk" required="true"/>
        </out-parameters>
    </service>
    <service verb="update" noun="TutorialEntity" displayName="Update an Example" type="entity-auto" >
        <in-parameters>
            <auto-parameters include="pk" required="true"/>
            <parameter name="tutorialName"/>
            <parameter name="tutorialDate"/>
            <parameter name="tutorialPrice"/>
            <parameter name="tutorialQuantity"/>
        </in-parameters>
    </service>
</services>